using VehicleDTO.Data;

namespace VehicleDTO.Models
{
   public VehicleModelList
   {

	IList<VehicleModelEntity> vehicleModelList = new List<VehicleModelEntity>();
   
        public static async Task<List<VehicleModelEntity>> GetVehiclesAsync()
  	{

			//var entityList = await _context.VehicleMakeEntity.ToListAsync();

			//var ent = new object();

			var entity = new VehicleModelEntity();

			entity.Id = 1;
			entity.Name = "Boris";
			entity.Abbrevation = "Boris";

			
	
			var list = new List<VehicleModelEntity>();

			list.Add(entity);

			return list;
			
	}		
    }

